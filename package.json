{
  "name": "gynergy-member-portal",
  "version": "1.0.0",
  "description": "Gynergy Member Portal MVP using Next.js 13, NextAuth, Supabase, & optional OpenAI Vision.",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
